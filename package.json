{
  "name": "vscode-moyu",
  "displayName": "摸鱼助手",
  "description": "将小说伪装成代码的VSCode插件",
  "version": "0.0.1",
  "publisher": "test",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-moyu.convertToCode"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-moyu.convertToCode",
        "title": "将小说转换为代码格式"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3"
  }
}
